    "import time\n",
    "import calendar\n",
    "k=0\n",
    "def nowtime():\n",
    "    t1=time.time()\n",
    "    localt1=time.localtime(t1)\n",
    "    localtime=time.asctime(localt1)\n",
    "    print(localtime)\n",
    "while k<=1:\n",
    "    k+=1\n",
    "    time.sleep(2)\n",
    "    nowtime()\n",
    "for i in range(1,13):\n",
    "    print(i,calendar.month(2019,i))\n",
    "    time.sleep(1)\n"
   ]
